{
  "name": "@t4y3/fig",
  "version": "0.2.1",
  "description": "Fig - library for gallery of Riot tag",
  "bin": {
    "fig-start": "index.js",
    "fig-build": "build.js"
  },
  "main": "infig/main.js",
  "scripts": {
    "start": "npm --prefix example/ run start",
    "build": "run-s build:js build:js:djt build:css",
    "build:js": "rollup -c",
    "build:js:djt": "rollup -c rollup.config.djt.js",
    "build:css": "node-sass src/scss -o dist/css",
    "watch": "run-p watch:*",
    "watch:js": "rollup -c -w",
    "watch:css": "node-sass src/scss -o dist/css -w",
    "lint": "run-p lint:*",
    "lint:js": "eslint src",
    "format": "run-s format:*",
    "format:js": "prettier-eslint --write 'src/**/*.js'",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier-eslint --write",
      "eslint",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/t4y3/fig.git"
  },
  "keywords": [
    "riot",
    "styleguide"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/t4y3/fig/issues"
  },
  "homepage": "https://github.com/t4y3/fig#readme",
  "dependencies": {
    "browser-sync": "^2.26.3",
    "fs-extra": "^7.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-transform-react-jsx": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "dom-json-tree": "^1.0.3",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-react": "^7.11.1",
    "esr": "^0.10.0",
    "husky": "^1.2.0",
    "hyperapp": "^1.2.9",
    "lint-staged": "^8.1.0",
    "node-sass": "^4.10.0",
    "npm-check-updates": "^2.14.3",
    "npm-run-all": "^4.1.3",
    "prettier-eslint-cli": "^4.7.1",
    "reset-css": "^4.0.1",
    "rollup": "^0.67.3",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-uglify": "^6.0.0",
    "siimple": "^3.1.1",
    "whatwg-fetch": "^3.0.0"
  }
}
