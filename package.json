{
  "name": "@t4y3/fig",
  "version": "0.3.0",
  "description": "Fig - library for gallery of Riot tag",
  "bin": {
    "fig-start": "index.js",
    "fig-build": "build.js"
  },
  "main": "infig/main.js",
  "scripts": {
    "start": "npm --prefix example/ run start",
    "build": "run-s build:js build:js:djt build:css",
    "build:js": "rollup -c",
    "build:js:djt": "rollup -c rollup.config.djt.js",
    "build:css": "node-sass src/scss -o dist/css",
    "watch": "run-p watch:*",
    "watch:js": "rollup -c -w",
    "watch:css": "node-sass src/scss -o dist/css -w",
    "lint": "run-p lint:*",
    "lint:js": "eslint src",
    "format": "run-s format:*",
    "format:js": "prettier-eslint --write 'src/**/*.js'",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier-eslint --write",
      "eslint",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/t4y3/fig.git"
  },
  "keywords": [
    "riot",
    "styleguide"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/t4y3/fig/issues"
  },
  "homepage": "https://github.com/t4y3/fig#readme",
  "dependencies": {
    "browser-sync": "^2.24.7",
    "fs-extra": "^7.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.2",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "dom-json-tree": "^1.0.3",
    "eslint": "^5.4.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-react": "^7.11.1",
    "esr": "^0.10.0",
    "highlight.js": "^9.12.0",
    "husky": "^0.14.3",
    "hyperapp": "^1.2.6",
    "hyperapp-render": "^2.1.0",
    "jest": "^23.5.0",
    "lint-staged": "^7.2.2",
    "node-sass": "^4.9.3",
    "npm-check-updates": "^2.14.2",
    "npm-run-all": "^4.1.3",
    "prettier-eslint-cli": "^4.7.1",
    "reset-css": "^4.0.1",
    "rollup": "^0.65.0",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.2.1",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-uglify": "^4.0.0",
    "siimple": "^3.1.1",
    "whatwg-fetch": "^2.0.4"
  }
}
